import java.util.Scanner;
public class Strings
{
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        System.out.print("Demoniators");
        String a=s.nextLine();
        String b="";
        int c=a.length();
        for(int i=c-1;i>=0;i--)
        {
            b=b+a.charAt(i);
        }
        if(a.equals(b))
            System.out.print("palindrome");
         else
            System.out.println("not palindrome");
            s.close();
    }
        
    
}
